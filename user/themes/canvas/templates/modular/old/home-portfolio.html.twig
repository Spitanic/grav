{{page.content|raw}}
<div id="portfolio" class="portfolio grid-container portfolio-full clearfix">
 {% for article in page.header.articles  %}
                 
     <article class="portfolio-item pf-media pf-icons">
        <div class="portfolio-image">
            <img src="{{root_url}}/{{article['file']|first['path']}}" alt="{{article['name']}}">
            <div class="portfolio-overlay">
                <a href="#" class="center-icon"><i class="icon-line-ellipsis"></i></a>
            </div>
        </div>
        <div class="portfolio-desc">
            <h3><a href="{{article['url']}}">{{article['name']}}</a></h3>
            <span>{{article['location']}}</span>
        </div>
    </article>
{%endfor %}
</div>